#include <iostream>
using namespace std;

int main(){
    int i,n,arr[1000];
    cin>>n;

    for( i=0;i<n;i++){
        cin >> arr[i];
    }
    for(i=1;i<n;i++){
    if(arr[0]<arr[i]){
        arr[0] = arr[i];
    }
    
    }
cout<< "Largest number in an array is"<<arr[0] << endl;
    return 0;
}
